<Window x:Class="Sérieathon.Seriathon"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sérieathon"
        mc:Ignorable="d"
        Title="Seriathon" Height="450" Width="800">

    <Window.Resources>
        <Style TargetType="Button" x:Key="styleTextBlockGlobal">
            <Setter Property="FontFamily" Value="Comic Sans MS"/>
        </Style>
    </Window.Resources>

    <DockPanel>
        <local:UC_Menu_Nav/>
        <ContentControl x:Name="content_control_page_accueil">
            <local:UC_accueil/>
        </ContentControl>
    </DockPanel>
</Window>
